@{
    ViewData["Title"] = "Home Page";
    var counter = 0;
}
@model HomeViewModel

<!-- SLIDER AREA START (slider-3) -->
<div class="ltn__slider-area ltn__slider-3  section-bg-1">
    <div class="ltn__slide-one-active slick-slide-arrow-1 slick-slide-dots-1">
        <!-- ltn__slide-item -->
        @foreach (var slide in Model.Sliders)
        {
            counter ++; 
        <div class="ltn__slide-item ltn__slide-item-2 ltn__slide-item-3-normal ltn__slide-item-3">
            <div class="ltn__slide-item-inner @(counter % 2 == 0? " text-right text-end":"")">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 align-self-center">
                            <div class="slide-item-info">
                                <div class="slide-item-info-inner ltn__slide-animation">
                                    <div class="slide-video mb-50 d-none">
                                        <a class="ltn__video-icon-2 ltn__video-icon-2-border" href="https://www.youtube.com/embed/tlThdr3O5Qo" data-rel="lightcase:myCollection">
                                            <i class="fa fa-play"></i>
                                        </a>
                                    </div>
                                    <h6 class="slide-sub-title white-color--- animated"><span><i class="fas fa-home"></i></span> Real Estate Agency</h6>
                                    <h1 class="slide-title animated ">@(Html.Raw(slide.Title))</h1>
                                    <div class="slide-brief animated">
                                        <p>@slide.Desc</p>
                                    </div>
                                    <div class="btn-wrapper animated">
                                       <a href="@slide.Btn1Url" class="theme-btn-1 btn btn-effect-1">@slide.Btn1Text</a>
                                        <a href="@slide.Btn2Url" class="btn btn-transparent btn-effect-3">@slide.Btn2Text</a> </div>
                                </div>
                            </div>
                                <div class="slide-item-img @(counter % 2 ==0? "slide-img-left":"") ">
                                <img src="~/Uploads/Sliders/@slide.ImageUrl" alt="#">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ltn__slide-item-end -->
            
        }
       
    </div>
</div>
<!-- SLIDER AREA END -->
