<div class="ltn__product-slider-area ltn__product-gutter pt-115 pb-90 plr--7">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <div class="section-title-area ltn__section-title-2--- text-center">
                    <h6 class="section-subtitle section-subtitle-2 ltn__secondary-color">Properties</h6>
                    <h1 class="section-title">Featured Listings</h1>
                </div>
            </div>
        </div>
        <div class="row ltn__product-slider-item-four-active-full-width slick-arrow-1">
            @foreach(var house in Model.Houses){
                <!-- ltn__product-item -->
              <div class="col-lg-12">
                <div class="ltn__product-item ltn__product-item-4 text-center---">
                    <div class="product-img">
                        <a href="product-details.html"><img src="~/Uploads//Houses/@house.HouseImages.FirstOrDefault(x=> x.PosterStatus==true)?.ImageUrl" alt="#"></a>
                        <div class="product-badge">
                            <ul>
                                <li class="sale-badge bg-green">For Rent</li>
                            </ul>
                        </div>
                        <div class="product-img-location-gallery">
                            <div class="product-img-location">
                                <ul>
                                    <li>
                                        <a href="locations.html"><i class="flaticon-pin"></i> @house.City.Name</a>
                                    </li>
                                </ul>
                            </div>
                            <div class="product-img-gallery">
                                <ul>
                                    <li>
                                        <a href="product-details.html"><i class="fas fa-camera"></i> @house.HouseImages.Count</a>
                                    </li>
                                    <li>
                                        <a href="product-details.html"><i class="fas fa-film"></i> @house.ParkingCount</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="product-info">
                        <div class="product-price">
                                <span>$@((house.Price *(100-house.DiscountPercent)/100).ToString("0.00"))</span>
                        </div>
                        <h2 class="product-title"><a href="product-details.html">@house.Title</a></h2>
                        <div class="product-description">
                            <p>
                                @house.PosterDesc
                            </p>
                        </div>
                        <ul class="ltn__list-item-2 ltn__list-item-2-before">
                            <li>
                                <span>@house.BedroomCount <i class="flaticon-bed"></i></span>
                                Bedrooms
                            </li>
                            <li>
                                <span>@house.BathroomCount <i class="flaticon-clean"></i></span>
                                Bathrooms
                            </li>
                            <li>
                                <span>@house.Area <i class="flaticon-square-shape-design-interface-tool-symbol"></i></span>
                                square Ft
                            </li>
                        </ul>
                    </div>
                    <div class="product-info-bottom">
                        <div class="real-estate-agent">
                            <div class="agent-img">
                                <a href="team-details.html"><img src="~/Uploads//Owners/@house.Owner.ImageUrl" alt="#"></a>
                            </div>
                            <div class="agent-brief">
                                    <h6><a href="team-details.html">@house.Owner.Fullname</a></h6>
                                <small>Estate Agents</small>
                            </div>
                        </div>
                        <div class="product-hover-action">
                            <ul>
                                <li>
                                    <a href="#" title="Quick View" data-bs-toggle="modal" data-bs-target="#quick_view_modal">
                                        <i class="flaticon-expand"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="#" title="Wishlist" data-bs-toggle="modal" data-bs-target="#liton_wishlist_modal">
                                        <i class="flaticon-heart-1"></i>
                                    </a>
                                </li>
                                <li>
                                    <a href="product-details.html" title="Product Details">
                                        <i class="flaticon-add"></i>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            }

            
          
        </div>
    </div>
</div>
